{
    "name": "bulton-fr/bfw-sql",
    "type": "library",
    "description": "SQL Module for BFW",
    "keywords": ["PDO", "SQL", "module", "bfw"],
    "homepage": "https://github.com/bulton-fr/bfw-sql",
    "time": "2016-11-05",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vermeulen Maxime / Bulton",
            "homepage": "http://www.bulton.fr",
            "email": "bulton.fr@bulton.fr"
        }
    ],
    "support": {
        "email": "support@bulton.fr",
        "issues": "https://github.com/bulton-fr/bfw-sql/issues",
        "source": "https://github.com/bulton-fr/bfw-sql"
    },
    "require": {
        "php": ">=5.6.0",
        "bulton-fr/bfw": "3.0.0-rc.7"
    },
    "require-dev": {
        "atoum/atoum": "~3.1",
        "atoum/visibility-extension": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "BfwSql\\": "src/class/",
            "BfwSql\\Executers\\": "src/class/executers/",
            "BfwSql\\Queries\\": "src/class/queries/",
            "BfwSql\\Queries\\Parts\\": "src/class/queries/parts/",
            "BfwSql\\Observers\\": "src/class/observers/",
            "BfwSql\\Runners\\": "src/class/runners/",
            "BfwSql\\Helpers\\": "src/helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BfwSql\\Test\\Helpers\\": "test/unit/helpers",
            "BfwSql\\Test\\Mocks\\": "test/unit/mocks/src/class",
            "BfwSql\\Actions\\Test\\Mocks\\": "test/unit/mocks/src/class/actions"
        }
    }
